// 키 바인딩을 이 파일에 넣어서 기본값 재정의
[
    {
        "key": "ctrl+cmd+p j",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "ctrl+`",
        "command": "-workbench.action.terminal.toggleTerminal",
        "when": "terminal.active"
    },
    {
        "key": "ctrl+cmd+p o",
        "command": "workbench.action.createTerminalEditor"
    },
    {
        "key": "ctrl+cmd+p s",
        "command": "workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "cmd+b",
        "command": "-workbench.action.toggleSidebarVisibility"
    },
    {
        "key": "ctrl+cmd+p l",
        "command": "workbench.action.decreaseViewSize"
    },
    {
        "key": "ctrl+cmd+p h",
        "command": "workbench.action.increaseViewSize"
    },
    {
        "key": "ctrl+cmd+p shift+h",
        "command": "workbench.action.focusLeftGroup"
    },
    {
        "key": "cmd+r cmd+left",
        "command": "-workbench.action.focusLeftGroup"
    },
    {
        "key": "ctrl+cmd+p shift+l",
        "command": "workbench.action.focusRightGroup"
    },
    {
        "key": "cmd+r cmd+right",
        "command": "-workbench.action.focusRightGroup"
    },
    {
        "key": "ctrl+cmd+p shift+cmd+f",
        "command": "workbench.action.experimental.quickTextSearch"
    },
    {
        "key": "ctrl+cmd+p ctrl+h",
        "command": "workbench.action.debug.continue",
        "when": "debugState == 'stopped'"
    },
    {
        "key": "ctrl+cmd+p ctrl+l",
        "command": "workbench.action.debug.stepOver",
        "when": "debugState == 'stopped'"
    },
    {
        "key": "ctrl+cmd+p ctrl+k",
        "command": "workbench.action.debug.stepOut",
        "when": "debugState == 'stopped'"
    },
    {
        "key": "ctrl+cmd+p ctrl+j",
        "command": "workbench.action.debug.stepInto",
        "when": "debugState != 'inactive'"
    }
]